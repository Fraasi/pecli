{
	"name": "pecli",
	"version": "0.6.1",
	"author": "Fraasi",
	"homepage": "https://github.com/fraasi/pecli",
	"repository": {
		"type": "git",
		"url": "https://github.com/fraasi/pecli.git"
	},
	"license": "MIT",
	"description": "node cli to inspect package.json",
	"bugs": {
		"url": "https://github.com/fraasi/pecli/issues"
	},
	"keywords": [
		"node",
		"cli",
		"command",
		"line",
		"package.json",
		"inspect",
		"explorer"
	],
	"main": "./lib/cli.js",
	"bin": "./lib/cli.js",
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"start": "node src/cli.js",
		"build": "env NODE_ENV=production babel src -d lib",
		"test-build": "node lib/cli.js",
		"test": "echo \"Error: no test specified\""
	},
	"files": [
		"lib/*"
	],
	"dependencies": {
		"import-jsx": "^3.0.0",
		"ink": "^2.5.0",
		"ink-select-input": "^3.1.2",
		"meow": "^5.0.0",
		"react": "^16.11.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@babel/register": "^7.6.2",
		"eslint": "^6.6.0",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.2.0"
	},
	"babel": {
		"presets": [
			"@babel/preset-react",
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": true
					}
				}
			]
		]
	}
}
